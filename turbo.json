{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"]
    },
    "lint": {
      "dependsOn": ["^lint"]
    },
    "lint:fix": {
      "dependsOn": ["^lint:fix"]
    },
    "fmt": {
      "dependsOn": ["^fmt"]
    },
    "fmt:check": {
      "dependsOn": ["^fmt:check"]
    },
    "test": {
      "dependsOn": ["^test"]
    },
    "dev": {
      "persistent": true,
      "cache": false
    },
    "clean": {
      "cache": false
    }
  }
}
