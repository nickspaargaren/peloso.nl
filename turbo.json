{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"]
    },
    "//#lint": {},
    "//#lint:fix": {
      "cache": false
    },
    "//#fmt": {
      "cache": false
    },
    "//#fmt:check": {},
    "test": {
      "dependsOn": ["^test"]
    },
    "dev": {
      "persistent": true,
      "cache": false
    },
    "clean": {
      "cache": false
    }
  }
}
